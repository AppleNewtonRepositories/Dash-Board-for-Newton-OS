ame: "A", cmdItem: {type: 'romapp, text: "App Name", data: 'appsymbol}},	{name: "B", cmdItem: {type: 'pkg, text: "App Name", data: <entry alias>}},	{name: "C", cmdItem: {type: 'cmd, text: "Command Name", data: 'cmdSymbol}},	{name: "D", cmdItem: {type: 'script, text: "Script Name", data: <entry alias to script soup>}},]//EXTRA SLOTS ONLY USED IF NECESSARY: